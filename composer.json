{
    "name": "versio/versio-php",
    "description": "Unofficial implementation of the Versio API.",
    "license": "MIT",
    "type": "library",
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Versio\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VersioTests\\": "tests"
        }
    },
    "require": {
        "php": "^8.1",
        "psr/http-client": "^1.0",
        "psr/http-client-implementation": "*",
        "psr/http-factory": "^1.0"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^7.5",
        "nyholm/psr7": "^1.5",
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.27"
    }
}
